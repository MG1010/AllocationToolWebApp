<div class="card-columns">
	<c:forEach  items="${team}" var="teammember">
	<div class="card p-3">
	  <!-- <img class="card-img-top" src="..." alt="Card image cap"> -->
	  <div class="card-body">
		  <h5 class="card-title">${teammember.firstName} ${teammember.lastName}</h5>
		  <h6 class="card-title">${teammember.position}</h6>
		  <%--<p>Tech stack: ${teammember.techstack}</p>--%>
		  <p class="card-text">${teammember.description}</p>
	  </div>
	</div>
	</c:forEach>
</div>